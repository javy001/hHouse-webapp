<div class="form-group">
<form (ngSubmit)="ngOnSubmit()">
      <md-input-container>
        <label>Water</label>
        <input mdInput [(ngModel)]="data.water" name="water">
      </md-input-container><br>
      <md-input-container>
        <label>Added Grow in ml</label>
        <input mdInput [(ngModel)]="data.grow" name="grow">
      </md-input-container><br>
      <md-input-container>
        <label>Added Micro in ml</label>
        <input mdInput [(ngModel)]="data.micro" name="micro">
      </md-input-container><br>
      <md-input-container>
        <label>Added Bloom in ml</label>
        <input mdInput [(ngModel)]="data.bloom" name="bloom">
      </md-input-container><br>
      <md-input-container>
        <label>Added Roots in ml</label>
        <input mdInput [(ngModel)]="data.roots" name="roots">
      </md-input-container><br>
      <md-input-container>
        <label>Hours of light?</label>
        <input mdInput [(ngModel)]="data.light" name="light">
      </md-input-container><br>
      <button md-raised-button type="submit" class="md-primary">Submit</button>
</form><br>
<md-card>
<p>{{ data.water  }}</p>
</md-card>
</div>